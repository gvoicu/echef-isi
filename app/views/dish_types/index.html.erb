<h1>Listing dish_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dish_types.each do |dish_type| %>
  <tr>
    <td><%= dish_type.name %></td>
    <td><%= link_to 'Show', dish_type %></td>
    <td><%= link_to 'Edit', edit_dish_type_path(dish_type) %></td>
    <td><%= link_to 'Destroy', dish_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

